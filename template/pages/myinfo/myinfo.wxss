.container {
    width: 100%;
    height: 100%;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    -webkit-box-orient: vertical;

}

.item-ul {
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;

    border-top: 1px solid #cccccc;
    margin-bottom: 30rpx;
    margin-top: -2px;

}

.item-li {
    display: flex;
    display: -webkit-flex;
    align-items: center;

    width: 100%;
    border-bottom: 1px solid #cccccc;
    height: 96rpx;
    padding-left: 24rpx;
    padding-right: 24rpx;
    background-color: #fff;
}

.item-right {
    position: absolute;
    right: 24rpx;
}

.item-title {
    font-size: 30rpx;
    color: #3b3b3b;
    font-weight: 450;
}

.item-content{
    font-size: 24rpx;
    color: #999999;
    margin-right: 20rpx;
}

.item-next {
    width: 17rpx;
    height: 29rpx;
}

.item-loyout {
    font-size: 30rpx;
    color: #fc5444;
    text-align: center;
    justify-content: center;
}
