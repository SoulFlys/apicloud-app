.container {
    width: 100%;
    height: 100%;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;
    -webkit-box-orient: vertical;
}

.scroll-view {
    width: 100%;
    height: 100%;
}

.list-con {
    width: 100%;
}

.list-cell {
    width: 100%;
    background-color: #ffffff;
    margin-bottom: 10rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-left: 24rpx;
    padding-right: 24rpx;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;
}

.cell-user {
    width: 100%;
    padding-top: 10rpx;
    padding-bottom: 20rpx;
}

.cell-pic{
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
}

.cell-userinfo{
    height:88rpx;
    display: flex;
    flex-direction: column;
    padding-top: 4rpx;
    padding-bottom: 4rpx;
    margin-left: 20rpx;
    box-sizing: border-box;
}

.cell-name{
    height: 36rpx;
    color:#3b3b3b;
    font-size: 30rpx;
    margin-top: 12rpx;
}

.cell-date{
    height: 32rpx;
    margin-top: 4rpx;
    color:#999999;
    font-size: 26rpx;
}

.cell-text{
    width: 100%;
}

.cell-intro{
    font-size: 26rpx;
    line-height: 22rpx;
    color: #3b3b3b;
}

.cell-imgs{
    width: 100%;
    padding-top: 20rpx;
    padding-bottom: 10rpx;

}

.cell-imgbox{
    display:flex;
    flex-wrap: wrap;
}

.cell-imgbox-1{
    width: 400rpx;
}
.cell-imgbox-4{
    width:380rpx;
}
.cell-imgbox-5,
.cell-imgbox-6{
    width: 570rpx;
}

.cell-img-1{
    width: 400rpx;
    height: 400rpx;
}
.cell-img-2,
.cell-img-3{
    width: 180rpx;
    height: 180rpx;
    margin-right: 10rpx;
}
.cell-img-4,
.cell-img-5,
.cell-img-6{
    width: 180rpx;
    height: 180rpx;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
}



.cell-prices{
    margin-top: 10rpx;
    width: 100%;
    padding-top: 22rpx;
    padding-bottom: 22rpx;
    padding-left: 24rpx;
    padding-right: 24rpx;
    border-top:1px solid #cccccc;
    border-bottom:1px solid #cccccc;
    border-right:1px solid #cccccc;
    border-left:2px solid #ff550c;
    display: flex;
    flex-direction: column;
}

.cell-prices-title{
    width: 100%;
    justify-content: space-between;
    display: flex;
    align-items: center;
}
.cell-prices-t{
    height: 30rpx;
    font-weight: 500;
    font-size: 26rpx;
    color:#3b3b3b;
}
.cell-prices-people{
    font-size: 22rpx;
    color: #a1a1a1;
}
.cell-people-icon{
    width:26rpx;
    height:24rpx;
    margin-right: 8rpx;
    margin-top: 2rpx;
}
.cell-num{
    font-size: 22rpx;
    color: #ff550c;
}
.cell-price{
    margin-top: 22rpx;
    height: 44rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.cell-price-item{
    align-items: flex-end;
}
.cell-price-item-2{
    padding-top: 8rpx;
}
.cell-price-1{
    font-size: 36rpx;
    color: #fe0036;
    font-weight: 500;
}
.cell-price-2{
    font-size: 24rpx;
    color: #a1a1a1;
}
.cell-price-3{
    font-size: 28rpx;
    color: #3b3b3b;
}
.cell-price-4{
    height: 38rpx;
    border-top: 1px solid #ff550c;
    border-bottom: 1px solid #ff550c;
    box-sizing: border-box;
    color: #ff550c;
    font-size: 22rpx;
    padding-top: 2rpx;
    padding-left: 4rpx;
    margin-left: -2px;
}
.cell-price-5{
    color: #ff550c;
    font-size:22rpx;
    border-radius: 24rpx;
    border: 1px solid #ff550c;
    height: 44rpx;
    padding-top: 6rpx;
    padding-left: 16rpx;
    padding-right: 16rpx;
}
.cell-quanleft{
    width: 18rpx;
    height: 38rpx;
    margin-top: 3rpx;
}
.cell-quanright{
    width: 10rpx;
    height: 38rpx;
    margin-top: 3rpx;
}


.cell-btns{
    display: flex;
    flex-direction: column;
    margin-top: 34rpx;
}

.cell-btn{
    width:100%;
    text-align: center;
    display: flex;
    justify-content:center;
}
.cell-share,
.cell-cli{
    font-size: 26rpx;
    color: #ff550c;
    height: 70rpx;
    border:1px solid #ff550c;
    width: 248rpx;
    text-align: center;
    line-height: 35rpx;
    background-color: #fff3ee;
}
.cell-share{
    border-top-left-radius: 35rpx;
    border-bottom-left-radius: 35rpx;
}
.cell-cli{
    border-top-right-radius: 35rpx;
    border-bottom-right-radius: 35rpx;
    border-left-width: 0;
}
.cell-tip{
    color: #999999;
    font-size: 26rpx;
    text-align: center;
    width: 100%;
    margin-top: 20rpx;
}
